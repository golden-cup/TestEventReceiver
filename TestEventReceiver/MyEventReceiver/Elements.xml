      <Name>EmployeeDocumentsLogItemAdding</Name>
      <Type>ItemAdded</Type>
      <Assembly>$SharePoint.Project.AssemblyFullName$</Assembly>
      <Class>TestEventReceiver.MyEventReceiver.MyEventReceiver</Class>
